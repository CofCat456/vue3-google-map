{
  "name": "@voomap/monorepo",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "taze": "taze major -wIr",
    "build": "pnpm --filter './packages/**' build",
    "build:map": "pnpm --filter @voomap/map build",
    "build:core": "pnpm --filter @voomap/core build",
    "clean": "rimraf --glob dist types \"packages/*/dist\"",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "docs:dev": "vitepress dev docs --port 2222",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 8080",
    "release": "bumpp -r && esno scripts/publish.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.14.0",
    "@antfu/ni": "^23.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.14",
    "@vitejs/plugin-vue": "^5.2.1",
    "bumpp": "^9.10.1",
    "consola": "^3.4.0",
    "eslint": "^9.18.0",
    "eslint-plugin-format": "^1.0.1",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.0",
    "lint-staged": "^15.4.1",
    "pnpm": "^9.15.4",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.11.1",
    "taze": "^18.2.0",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-replace": "^0.1.1",
    "vite-plugin-style-inject": "^0.0.1",
    "vitepress": "1.0.0-beta.7",
    "vue": "^3.5.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
